{
  "name": "eslint-plugin-capo",
  "version": "0.1.0",
  "description": "ESLint plugin to validate HTML <head> elements based on capo.js rules",
  "main": "src/index.js",
  "type": "module",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "html",
    "head",
    "validation",
    "capo",
    "performance",
    "seo"
  ],
  "author": "Rick Viscomi",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rviscomi/eslint-plugin-capo.git"
  },
  "peerDependencies": {
    "@html-eslint/parser": ">=0.47.0",
    "eslint": ">=8.21.0"
  },
  "devDependencies": {
    "@html-eslint/parser": "^0.47.0",
    "dedent": "^1.7.0",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "node --test tests/rules/*.test.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,md,json,html}": "prettier --write"
  }
}
